  SOURCE tremor/res012.c
  SOURCE tremor/sharedbook.c
  SOURCE tremor/window.c
  HEADER tremor/ivorbiscodec.h
  HEADER tremor/codec_internal.h
  HEADER tremor/misc.h
  EXPINCLUDE tremor
