#include "*/*.proj"

CON(CONFIG_EBML_WRITING) mkclean
{
  PROJECT_NAME "mkclean"
  PROJECT_VERSION  0.4.4
  PROJECT_VENDOR   "Matroska Foundation"
  USE matroska2
  IF !CONFIG_EBML_UNICODE
    USE parser
  ENDIF
  SOURCE mkclean.c
  COMPILE src.br
  COMPILE configure
  COMPILE pkg.sh
}


