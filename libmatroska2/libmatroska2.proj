#include "*/*.proj"

PROJECT matroska2
{
  PROJECT_NAME  "libmatroska2"
  PROJECT_VERSION  0.9.2
  PROJECT_VENDOR   "Matroska Foundation"
}

GROUP matroska2_group
{
  USE(HAVE_EBML2) ebml2

  INCLUDE .
  EXPINCLUDE .
  
  SOURCE matroskamain.c {class Matroska_Class}
  HEADER matroska/matroska.h
}

DLL matroska2
{
  USE matroska2_group
}
